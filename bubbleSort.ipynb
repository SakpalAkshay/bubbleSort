    "#bubble sort\n",
    "def bubbleSort(arr):\n",
    "    for i in range(len(arr)-1):\n",
    "        for j in range(0,len(arr)-i-1):\n",
    "            if arr[j] > arr[j+1]:\n",
    "                arr[j],arr[j+1]= arr[j+1],arr[j]\n",
    "                    \n",
    "arr = [9,1,4,5,17,24,96,38,0,111]\n",
    "bubbleSort(arr)\n",
    "print(arr)\n",
    "        "
   ]
